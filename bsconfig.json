{
  "name": "my-new-project",
  "version": "0.1.0",
  "generators": [
    {
      "name": "atd_t",
      "command": "yarn run atdgen -t $in"
    }
  ],
  "sources": {
    "dir" : "src",
    "subdirs" : true,
    "generators": [
      {
        "name": "atd_t",
        "edge": ["Foo_t.ml", "Foo_t.mli", ":", "Foo.atd"]
      }
    ]
  },
  "package-specs": {
    "module": "commonjs",
    "in-source": true
  },
  "suffix": ".bs.js",
  "bs-dependencies": [],
  "warnings": {
    "error" : "+101"
  },
  "namespace": true,
  "refmt": 3
}

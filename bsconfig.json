{
  "name": "my-new-project",
  "version": "0.1.0",
  "generators":  [
    {
      "name": "atd_t",
      "command": "part1=$$(dirname $in) && part2=$$(basename $in .atd) && yarn run atdgen -t $in -o $$part1/../gen/$$part2"
    }
  ],
  "sources": [
    "src",
    {
      "dir": "gen",
      "generators": [
        {
          "name": "atd_t",
          "edge": ["Foo_t.ml", "Foo_t.mli", ":", "../src/Foo.atd"]
        }
      ]
    }
  ],
  "package-specs": {
    "module": "commonjs",
    "in-source": true
  },
  "suffix": ".bs.js",
  "bs-dependencies": [],
  "warnings": {
    "error": "+101"
  },
  "namespace": true,
  "refmt": 3
}
